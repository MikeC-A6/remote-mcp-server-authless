# Cloudflare Environment Variables
# Copy this file to .env for local development

# Your Cloudflare Account ID (found in dashboard URL or right sidebar)
CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# Your Cloudflare API Token (create at https://dash.cloudflare.com/profile/api-tokens)
# Needs "Cloudflare AI:Read" permission
CLOUDFLARE_API_TOKEN=your_api_token_here 